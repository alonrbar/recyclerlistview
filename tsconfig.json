{
  "include": [
    "src"
  ],
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "jsx": "react",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "removeComments": false,
    "rootDir": "src",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "es5",
    "types": ["react", "react-native"]
  }
}